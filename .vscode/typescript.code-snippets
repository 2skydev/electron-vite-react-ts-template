      "import clsx from 'clsx';\n",
      "import { ${TM_DIRECTORY/^.+\\/(.*)$/$1/}Styled } from './styled';\n",
      "interface ${TM_DIRECTORY/^.+\\/(.*)$/$1/}Props {",
      "  className?: string;",
