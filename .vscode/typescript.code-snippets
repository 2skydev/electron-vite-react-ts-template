      "  return (",
      "    <${TM_DIRECTORY/^.+\\/(.*)$/$1/}Styled className={clsx('${TM_DIRECTORY/^.+\\/(.*)$/$1/}', className)}>",
      "      $0",
      "    </${TM_DIRECTORY/^.+\\/(.*)$/$1/}Styled>",
      "  )",
      "}\n",
      "export default ${TM_DIRECTORY/^.+\\/(.*)$/$1/}"
    ],
